<resources>

    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="view_med_main">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/button_edit_height</item>
        <item name="android:textColor">#333</item>
        <item name="android:textColorHint">#777</item>
        <item name="android:textSize">@dimen/normal_text</item>
        <item name="android:elevation">2dp</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:layout_marginBottom">8dp</item>
        <item name="android:layout_marginLeft">8dp</item>
        <item name="android:layout_marginRight">8dp</item>

    </style>

    <style name="med_edit_text" parent="view_med_main">
        <item name="android:background">@drawable/rounded_edit_text</item>

    </style>

</resources>

